<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.develop.filhan.eencyclopediaone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListItem" />
        <activity android:name=".SearchItem" />
        <activity android:name=".Splashscreen" />
        <activity android:name=".DetailItem" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_home" />
        <activity android:name=".SignupActivity" android:parentActivityName=".HomeActivity"></activity>
    </application>

</manifest>